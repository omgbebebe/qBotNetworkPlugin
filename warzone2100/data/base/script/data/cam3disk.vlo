// cam3disk.vlo
// Weather and day/night VLO for Cam3
// set nighttime
// change to a different CD

script "cam3daynight.slo"
run
{
//night time colour for fog ROCKIES
Rstart		int	18
Gstart		int	23
Bstart		int	24

//night time colour for fog
Rend		int	18
Gend		int	23
Bend		int	24

Steps		int	1	//number of seconds for change
disk		int	1	//change back to CD#1
}